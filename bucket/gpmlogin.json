{
    "version": "0.1.7",
    "description": "Antidetect browser",
    "homepage": "https://gpmloginapp.com",
    "license": "Proprietary",
    "depends": "innounp",
    "architecture": {
        "64bit": {
            "url": "https://assets.giaiphapmmo.net/gpmloginglobal/GPMLoginGlobal_setup_latest.exe",
            "hash": "4b5703331604cf4a9972fba107ac321c5e46ae9d20e0e3e07a1ac7130787a20f"
        }
    },
    "pre_install": [
        "innounp -x \"$dir\\$fname\" -d\"$dir\" -p\"gpmsoftwares.com\"",
        "Move-Item \"$dir\\{app}\\*\" \"$dir\""
    ],
    "post_install": "Remove-Item \"$dir\\{app}\", \"$dir\\$fname\" -Recurse -Force",
    "shortcuts": [
        [
            "GPMLoginGlobal.exe",
            "GPMLoginGlobal"
        ]
    ],
    "checkver": {
        "script": [
            "$dl_url = 'https://assets.giaiphapmmo.net/gpmloginglobal/GPMLoginGlobal_setup_latest.exe'",
            "$dl = cache_path 'gpmlogin' 'unknown' $dl_url",
            "Invoke-WebRequest $dl_url -OutFile $dl",
            "$ver = (Get-Item $dl).VersionInfo.ProductVersion",
            "Move-Item -Force $dl (cache_path 'gpmlogin' $ver $dl_url)",
            "$ver"
        ],
        "regex": "([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://assets.giaiphapmmo.net/gpmloginglobal/GPMLoginGlobal_setup_latest.exe"
            }
        }
    }
}
